# Algoritma
# Contoh List 
nama = ["Yemima", "Saleha", "Ana", "Rofi"]

#Mengubah isi list
nama[1] = "Tia"

#Menambah data
nama.append("Aura") 

print(nama) 


# Contoh Tuple
buah = ("Apel", "Anggur", "Jeruk") 

print(buah) 


# Contoh List dan Tuple bersamaan
data_mahasiswa = ("Kristia", 19,
["D0425323", "Sisfo A"]) 

# List di dalam tuple bisa diubah, tetapi tuple-nya tetap tidak bisa diubah
data_mahasiswa[2].append("Pemrograman") 

print(data_mahasiswa) 
